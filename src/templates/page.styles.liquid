  <h2 id="docs-colors" class="docs-title">Colors</h2>

  {% assign gray_scale_colors = '' %}
  {% assign gray_scale_colors = gray_scale_colors | append: 'black:Black:#121212:$black' | append: '|' %}
  {% assign gray_scale_colors = gray_scale_colors | append: 'gray:Gray:#424446:$gray' | append: '|' %}
  {% assign gray_scale_colors = gray_scale_colors | append: 'white:White:#fff:$color-white' | append: '|' %}
  {% assign gray_scale_colors = gray_scale_colors | split: '|' %}

  <h4 class="docs-subtitle">Grayscale</h4>
  <div>
    {% for color in gray_scale_colors %}
      {% assign parts = color | split: ':' %}
      <div class="styleguide-color">
        <div class="styleguide-color__swatch styleguide-color__swatch--{{ parts[0] }}"></div>
        <p class="text-p6-m lg:text-p6">{{ parts[1] }}<br />{{ parts[2] }}<br /><code>{{ parts[3] }}</code></p>
      </div>
    {% endfor %}
  </div>

  {% assign main_colors = '' %}
  {% assign main_colors = main_colors | append: 'green:Green:#4a5d36:$green' | append: '|' %}
  {% assign main_colors = main_colors | append: 'blue:Blue:#2c6275:$blue' | append: '|' %}
  {% assign main_colors = main_colors | append: 'brown:Brown:#452b2b:$brown' | append: '|' %}
  {% assign main_colors = main_colors | append: 'tan:Tan:#c3baa8:$tan' | append: '|' %}
  {% assign main_colors = main_colors | append: 'red:Red:#ff2b00:$red' | append: '|' %}
  {% assign main_colors = main_colors | append: 'cyan:Cyan:#009dcc:$cyan' | append: '|' %}
  {% assign main_colors = main_colors | append: 'yellow:Yellow:#ccff00:$yellow' | append: '|' %}

  {% assign main_colors = main_colors | split: '|' %}

  <h4 class="docs-subtitle">Main</h4>
  <div>
    {% for color in main_colors %}
