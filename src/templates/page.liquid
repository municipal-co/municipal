<div class="layout-content-wrapper">
  <div class="container-fluid">
      <div class="row justify-content-center">
        <div class="page-text-only col-xl-22 col-lg-22 col-md-22 col-sm-20 col-20">
          <div class="page-text-only__header">
            <h5>{{ page.metafields.custom_fields["page_subheader"] }}</h5>
            <h1>{{ page.title }}</h1>
            <p class="p2">{{ page.metafields.custom_fields["page_subtitle"] }}</p>
          </div>
          <div class="page-text-only__content">
            {{ page.content }}
          </div>
        </div>  
      </div>
  </div>
</div>
